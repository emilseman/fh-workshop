{
  "name": "integrator",
  "version": "0.0.0",
  "description": "Integrator with feature hub",
  "keywords": [
    "feature-hub"
  ],
  "author": "Emil Seman <emil.seman@sinnerschrader.com>",
  "homepage": "",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "scripts": {
    "build": "run-p build:cjs build:es",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:es": "tsc --project tsconfig.build.es.json",
    "build:watch": "yarn build:cjs --watch --preserveWatchOutput --pretty",
    "server": "nodemon dist",
    "watch": "run-p build:watch server"
  },
  "dependencies": {
    "@feature-hub/core": "^2.4.0",
    "@feature-hub/module-loader-amd": "^2.4.0",
    "@feature-hub/react": "^2.4.0",
    "express": "^4.17.1",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
